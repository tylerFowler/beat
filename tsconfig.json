{
  "files": [
    "lib/main.tsx",
    "lib/apiCacheWorker.ts"
  ],
  "exclude": [
    "public",
    "**/*.spec.ts"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES6",
    "lib": [
      "es6",
      "dom",
      "es2017.string",
      "es2017.object",
      "webworker",
    ],
    "types": [
      "node",
      "react",
      "react-dom",
      "react-redux"
    ],
    "paths": {
      "lib/styled-components": [ "lib/styled-components.ts" ],
      "panel/styled-components": [ "lib/panel/styled-components.ts" ]
    },
    "typeRoots": [ "node_modules/@types", "node_modules" ],
    "outDir": "build/",
    "sourceMap": true,
    "module": "ES6",
    "moduleResolution": "node",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictFunctionTypes": false,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react"
  }
}

{
  "name": "chrome-dashboard",
  "description": "A Chrome new tab page featuring HN/DN news feeds and other customizations",
  "author": "Tyler Fowler <tylerfowler.1337@gmail.com>",
  "version": "0.0.0",
  "main": "lib/main.tsx",
  "scripts": {
    "build": "npx rollup -c rollup.config.js",
    "lint": "npx tslint ./lib/*** --format stylish",
    "start-dev": "rollup -c rollup-devserver.config.js --watch",
    "build-assets": "npx imagemin 'assets' --out-dir 'public/assets'",
    "build-dist": "NODE_ENV=production npm run build && npm run build_assets"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "styled-components": "^4.1.3",
    "typesafe-actions": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.12",
    "@types/styled-components": "^4.1.4",
    "imagemin-cli": "^4.0.0",
    "redux-devtools-extension": "^2.13.7",
    "rollup": "^1.0.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy-glob": "^0.2.2",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^0.6.1",
    "rollup-plugin-tslint": "^0.1.38",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-watch": "^4.3.1",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}

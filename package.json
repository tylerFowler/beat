{
  "name": "chrome-dashboard",
  "description": "A Chrome new tab page featuring HN/DN news feeds and other customizations",
  "author": "Tyler Fowler <tylerfowler.1337@gmail.com>",
  "version": "0.0.0",
  "main": "lib/main.tsx",
  "scripts": {
    "build": "npx rollup -c rollup.config.js",
    "lint": "npx tslint ./lib/*** --format stylish",
    "start-dev": "rollup -c rollup-devserver.config.js --watch",
    "build-assets": "npx imagemin 'assets' --out-dir 'public/assets'",
    "build-dist": "NODE_ENV=production npm run build && npm run build_assets"
  },
  "dependencies": {
    "natural-scale": "^2.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "styled-components": "^4.1.3",
    "typesafe-actions": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.1",
    "@types/styled-components": "^4.1.11",
    "imagemin-cli": "^4.0.0",
    "redux-devtools-extension": "^2.13.8",
    "rollup": "^1.4.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-copy-glob": "^0.3.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.19.3",
    "rollup-watch": "^4.3.1",
    "tslib": "^1.9.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  }
}
